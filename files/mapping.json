PUT projects
{
  "mappings":{
    "_doc": {
      "properties": {
        "type": {"type":"text"},
        "title": {"type":"text"},
        "abstract": {"type":"text"},
        "notes": {"type":"text"},
        "urls": {"type":"text"},
        "status": {"type":"keyword"},
        "funding": {"type":"integer"},
        "start": {
          "type":"date",
          "format":"yyyy-MM-dd"
        },
        "expected_complete": {
          "type":"date",
          "format":"yyyy-MM-dd"
        },
        "actual_complete": {
          "type":"date",
          "format":"yyyy-MM-dd"
        },
        "performing_agencies":{
          "type":"nested",
          "properties": {
            "name": {"type":"text"},
            "city": {"type":"text"},
            "state": {"type":"keyword"},
            "country": {"type":"keyword"},
            "postal_code": {"type":"text"},
            "site_url": {"type":"text"}
          }
        },
        "funding_agencies": {
          "type":"nested",
          "properties": {
            "name": {"type":"text"},
            "city": {"type":"text"},
            "state": {"type":"keyword"},
            "country": {"type":"keyword"},
            "postal_code": {"type":"text"},
            "site_url": {"type":"text"}
          }
        },
        "managing_agencies": {
          "type":"nested",
          "properties": {
            "name": {"type":"text"},
            "city": {"type":"text"},
            "state": {"type":"keyword"},
            "country": {"type":"keyword"},
            "postal_code": {"type":"text"},
            "site_url": {"type":"text"}
          }
        },
        "project_investigators": {
          "type":"nested",
          "properties": {
            "first_name":{"type":"keyword"},
            "last_name":{"type":"keyword"}
          }
        },
        "responsible_individuals": {
          "type":"nested",
          "properties": {
            "first_name":{"type":"keyword"},
            "last_name":{"type":"keyword"}
          }
        },
        "TRID_RECORD_BASE": {"type":"text"},
        "TRID_RECORD_ID": {"type":"keyword"},
        "TRID_RECORD_TYPE": {"type":"keyword"},
        "TRID_TRIS_FILE_CODES": {"type":"keyword"},
        "TRID_INDEX_TERMS": {"type":"keyword"},
        "TRID_SUBJECT_AREAS": {"type":"keyword"}
      }
    }
  }
}

PUT trid-publications
{
  "mappings":{
    "publication": {
      "properties": {
        "title": {"type":"text"},
        "abstract": {"type":"text"},
        "notes": {"type":"text"},
        "urls": {"type":"text"},
        "authors": {
          "type":"nested",
          "properties": {
            "first_name":{"type":"keyword"},
            "last_name":{"type":"keyword"},
            "position":{"type":"integer"}
          }
        },
        "serial":{"type":"text"},
        "serial_url":{"type":"text"},
        "publisher":{"type":"text"},
        "publication_date":{
          "type":"date",
          "format":"yyyy-MM-dd"
        },
        "TRID_RECORD_BASE": {"type":"text"},
        "TRID_RECORD_ID": {"type":"keyword"},
        "TRID_RECORD_TYPE": {"type":"keyword"},
        "TRID_TRIS_FILE_CODES": {"type":"keyword"},
        "TRID_INDEX_TERMS": {"type":"keyword"},
        "TRID_SUBJECT_AREAS": {"type":"keyword"}
      }
    }
  }
}